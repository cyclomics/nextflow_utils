nextflow_process {

    name "Test Process Minimap2AlignAdaptiveParameterized"
    script "parse_convert/modules/minimap.nf"
    process "Minimap2AlignAdaptiveParameterized"
    
    
    test("Minimap test output") {

        when {
            process {
                """
                fq_file = file("$baseDir/example_datasets/example_consensus_reads/reads/ABC0001_0.fastq")
                ref = file("$baseDir/example_datasets/reference/pJPVCS_BBCS.mmi")

                input[0] = Channel.of(fq_file)
                input[1] = Channel.of(ref)
                """
            }
        }
        then {
            assert process.success
            assert snapshot(process.out).match()

            // with(process.out[0]) {
            //     assert path(get(0)[1]).md5 == "6dff2faaa2f7a1ae3b1d568c71f26ed8"
            // }
        }
    }
}
